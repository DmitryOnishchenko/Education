<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="student" class="com.donishchenko.springtest.Student">
        <property name="name" value="Test"/>
        <property name="age" value="11"/>
    </bean>

    <bean id="message1" class="com.donishchenko.springtest.Message">
        <constructor-arg name="message" value="Test message 1"/>
    </bean>

    <bean id="message2" class="com.donishchenko.springtest.Message">
        <constructor-arg name="message" value="Test message 2"/>
    </bean>
</beans>